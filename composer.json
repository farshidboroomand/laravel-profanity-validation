{
    "name": "farshidboroomand/laravel-profanity-validation",
    "description": "Filters out profanities from strings using validation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Farshid Boroomand",
            "email": "farshidboroomand@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "laravel/pint": "^1.5"
    },
    "scripts": {
        "lint": "pint --test",
        "lint:fix": "pint",
        "test": "phpunit ./tests",
        "test:u": "phpunit ./tests -d --update-snapshots"
    },
    "autoload": {
        "psr-4": {
            "Farshidboroomand\\ProfanityValidation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Farshidboroomand\\ProfanityValidation\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}

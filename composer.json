{
    "name": "owowagency/laravel-profanity-validation",
    "description": "Filters out profanities from strings using validation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Amine Farhaoui",
            "email": "amine@owow.io"
        },
        {
            "name": "Dees Oomens",
            "email": "dees@owow.io"
        },
        {
            "name": "Dean Tedesco",
            "email": "dino@tedesco.net.au"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "laravel/pint": "^1.5"
    },
    "scripts": {
        "lint": "pint --test",
        "lint:fix": "pint",
        "test": "phpunit ./tests",
        "test:u": "phpunit ./tests -d --update-snapshots"
    },
    "autoload": {
        "psr-4": {
            "OwowAgency\\ProfanityValidation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwowAgency\\ProfanityValidation\\ServiceProvider"
            ]
        }
    }
}
